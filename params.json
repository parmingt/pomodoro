  "body": "\r\n<!DOCTYPE html>\r\n\r\n<script src=\"https://code.jquery.com/jquery-2.2.4.min.js\"></script>\r\n<!-- Latest compiled and minified CSS -->\r\n<link rel=\"stylesheet\" href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css\" integrity=\"sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7\" crossorigin=\"anonymous\">\r\n\r\n<!-- Optional theme -->\r\n<link rel=\"stylesheet\" href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css\" integrity=\"sha384-fLW2N01lMqjakBkx3l/M9EahuwpSfeNvV63J5ezn3uZzapT0u7EYsXMjQV+0En5r\" crossorigin=\"anonymous\">\r\n\r\n<!-- Latest compiled and minified JavaScript -->\r\n<script src=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js\" integrity=\"sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS\" crossorigin=\"anonymous\"></script>\r\n<script type=\"text/javascript\">\r\n$(document).ready(function() {\r\n  $(\".project\").addClass(\"img-rounded col-xs-5\");\r\n  $(\".project:odd\").addClass(\"right\");\r\n  $(\".project:even\").addClass(\"left\");\r\n});\r\n$(window).scroll(function() {\r\n  if ($(window).scrollTop() > $(\".opener\").offset().top) {\r\n    $(\".navbar\").css(\"opacity\", \"0.50\");\r\n    $(\".navbar\").mouseover(function() {\r\n      $(\".navbar\").css(\"opacity\", \"1.0\");\r\n    });\r\n    $(\".navbar\").mouseout(function() {\r\n      $(\".navbar\").css(\"opacity\", \"0.5\");\r\n    });\r\n  } else {\r\n    $(\".navbar\").css(\"opacity\", \"1.0\");\r\n  }\r\n});\r\n$(\".navbutton\").mouseover(function(){\r\n  $(this).addClass(\"highlight\");\r\n});\r\n$(\".navbutton\").mouseout(function(){\r\n  $(this).removeClass(\"highlight\");\r\n});\r\n</script>\r\n<style type=\"text/css\">\r\n  body {\r\n  background-color:#2B2B2B;\r\n}\r\nh1 {\r\n  font-size: 4vw;\r\n}\r\nh2 {\r\n  font-size: 2vw;\r\n}\r\n.opener {\r\n  margin-top: 50px;\r\n  display:inline-block;\r\n  background-size: 100%;\r\n  width: 100%;\r\n  background-repeat: no-repeat;\r\n  z-index: 0;\r\n  height: 0;\r\n  padding-bottom: 45%;\r\n  border-top:1px solid white;\r\n  border-bottom:1px solid white;\r\n}\r\n.navbar {\r\n  background-color:#2B2B2B;\r\n  position: fixed;\r\n  opacity: 1;\r\n  text-align: right;\r\n  z-index: 1;\r\n  width: 100%;\r\n}\r\n.intro {\r\n  float: right;\r\n  text-align: right;\r\n  width: 50%;\r\n  color: white;\r\n  padding-right: 2%;\r\n  text-shadow: 2px 2px #000;\r\n}\r\n.portfolio {\r\n  width: 100%;\r\n  background-color: #A2AB58;\r\n  margin-top:10px;\r\n  margin-bottom:10px;\r\n  border-top:1px solid white;\r\n  border-bottom:1px solid white;\r\n}\r\n.project {\r\n  border: 1px solid white;\r\n  background-color:#2B2B2B;\r\n  height: 300px;\r\n  border-stlye: solid;\r\n  margin-left: 30px;\r\n  margin-right: 30px;\r\n  margin-bottom:30px;\r\n  padding: 5px;\r\n}\r\n.right {\r\n  float: right;\r\n}\r\n.left {\r\n  float: left;\r\n}\r\n.navbutton {\r\n  background-color: #E44424;\r\n}\r\n.contactBox {\r\n  background-color: #67BCDB;\r\n  height: 400px;\r\n  border-top:1px solid white;\r\n  border-bottom:1px solid white;\r\n}\r\n.contactForm {\r\n  text-align: left;\r\n  display: flex;\r\n  flex-direction: column;\r\n  padding-left:10px;\r\n}\r\n.social {\r\n}\r\n.contactlinks {\r\n  display: inline-block;\r\n}\r\n.socialButton {\r\n  border-radius: 50%;\r\n  width:50px;\r\n  height:50px;\r\n  background-size:100%;\r\n}\r\n.highlight {\r\n  border-width: 4px;\r\n  border-style: solid;\r\n}\r\n.preview{\r\n  height:100%;\r\n  width:100%;\r\n}\r\n.contactInput {\r\n  width:70%;\r\n  margin-bottom:4px;\r\n}\r\nh1 {\r\n  padding-top: 10px;\r\n  padding-bottom: 10px;\r\n  border-top : 1px dashed white;\r\n  border-bottom : 1px dashed white;\r\n}\r\n</style>\r\n<html>\r\n  <body>\r\n  <div class=\"navbar container\" align=\"center\" id=\"navbar\">\r\n\r\n  <a href=\"#top\" style=\"color:#2B2B2B\"><button class=\"navbutton btn\">Introduction</button></a>\r\n\r\n  <a href=\"